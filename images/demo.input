### Git car demo
cd /tmp
mkdir test.git
cd test.git
git init --bare
cd ..
git clone test.git
cd test
touch .gitignore
git add -A
git commit -am 'First commit'
git push
touch README
git add -A
git commit -am 'Adding README'
git push
git tag v1.0.0
git checkout v1.0.0
git checkout HEAD~1
git checkout master
git reset --hard HEAD~1
git commit --amend -am "Amending"
cd ~
rm -fr /tmp/test{,.git}
clear
